white_stuff = [0]*16
for i in range(8):
    white_stuff[i] = Pawn([1, i], 1)

white_stuff[8] = Rook([0,0], 1)
white_stuff[9] = Rook([0,7], 1)
white_stuff[10] = Knight([0,1], 1)
white_stuff[11] = Knight([0,6], 1)
